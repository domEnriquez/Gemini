@model IEnumerable<HMSWebApp.ViewModels.VoteEntryViewModel>

@foreach (var item in Model) {
    <div>
        <input type="hidden" value=@item.VoteEntryId />
        <h4>@item.VoterLastName @item.VoterFirstName</h4>
        <h4>@item.VoterEmailAddress</h4>
        <h4>@item.VoteEntryType</h4>
        <h4>@item.PaymentAmount</h4>
        <h4>@item.PaymentCurrency</h4>
        <h4>@item.PaymentPesoEquivalent</h4>
        @Html.ActionLink("Delete", "Delete", new { voteEntryId=item.VoteEntryId })
        @Html.ActionLink("Edit", "Edit", new { voteEntryId=item.VoteEntryId })
    </div>
    <hr />
}
